{
    "name": "piwi/composer-symlinker",
    "type": "composer-plugin",
    "description": "A package symlinker for local dev environment.",
    "require": {
        "composer-plugin-api": "1.*"
    },
    "require-dev": {
        "composer/composer": "1.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "ComposerSymlinker": "src/" }
    },
    "extra": {
        "class": "ComposerSymlinker\\SymlinkerPlugin"
    }
}
